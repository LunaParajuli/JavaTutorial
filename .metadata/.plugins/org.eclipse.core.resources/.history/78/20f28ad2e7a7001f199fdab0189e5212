//Write a program to print 10 even numbers and 10 odd numbers.


package WhileLoop;

public class WhileLoop1 {
 public static void main(String args[]) {
	 int number=0;
	 int evenCount=0;
	 System.out.println("First 10 even numbers are: ");
	 while(evenCount < 10) {
		if(number%2==0) {
			System.out.println(num+"");
		}
		number++
	 }
 }
}
