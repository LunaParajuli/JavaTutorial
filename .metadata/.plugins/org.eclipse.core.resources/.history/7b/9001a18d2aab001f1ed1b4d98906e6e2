// Write a program to generate 10 Fibonacci numbers

package DoWhile;

public class DoWhile6 {
	
	public static void main(String args[]) {
		
		 int count = 1; 
	        int a = 0;    
	        int b = 1;

	        System.out.println("The first 10 Fibonacci numbers are:");
	        
	        do {
	            int next = a + b; 
	            System.out.print(next); 
	            a = b;          
	            b = next;       
	            count++;        
	        } while (count < 9); 
	}

}
