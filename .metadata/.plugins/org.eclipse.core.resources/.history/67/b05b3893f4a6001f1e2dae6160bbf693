//Write a program to print 10 even numbers and 10 odd numbers.

package ForLoop;

import java.util.*;

public class Loop1 {
	public static void main(String args[]) {
		int i, even, odd;
		
		for( i=0; i<=10; i++) {
		 if(i%2 == 0 ) {
			 even = i;
		 }
		 else {
			 odd = i;
		 }
		}
		System.out.println("The even numbers are: ");
		
	}

}
