package Datatypes;

import java.util.*;

public class Datatype1 {
  public static void main(String args[]) {
	 
	  Scanner sc = new Scanner(System.in);
	  System.out.println("Enter the value of a:");
	  int a = sc.nextInt();
	  
	 
	  
	
	  System.out.println("The value of a:"+a);
  }
}
