//Write a program to generate 10 Fibonacci numbers.

package ForLoop;

import java.util.*;

public class Loop5fibonacci {
public static void main(String args[]) {
	int a=0,b=1,current,temp;
	int num=10;
	
	for(int i=0;i<=10;i++) {
		current=a+b;
		a=b;
		b=a;
	}
	System.out.println+"The 10 fibonacci numbers are: "+ current);
}
}
