//Write a program to print 10 even numbers and 10 odd numbers.

package ForLoop;

import java.util.*;

public class Loop1 {
	public static void main(String args[]) {
		int even , odd;
		int num=1;
		for(int i=0; i<=10; i++) {
			 even =  i*2;
			 odd  = i*2+1;
			 
			 System.out.println("The even numbers are:" + even);
			 System.out.println("The odd numbers are:" + odd);
		}
		
	}

}
