// Write a program to generate 10 Fibonacci numbers.

package WhileLoop;



public class WhileLoop6 {
  public static void main(String args[]) {
	  
	  int a=1;
	  int b=1 ;
	  int current;
	  int i=0;
	  
	  System.out.println("The 10 fibonacci numbers are:");
	  while(i < 10) {
		  current = a+b;
		  a=b;
		  b=a;
		  System.out.println(current);
	  }
	  
  }
}
