//Write a program to find largest of two numbers.
 
package IfElse;

import java.util.*;

public class IfCondition3 {
public static void main(String args[]) {
	int  a , b;
	Scanner sc = new Scanner(System.in);
	
   a = sc.nextInt();
   System.out.println("Enter the value of a: "+a);
	
   b= sc.nextInt();
   System.out.println("Enter the value of b:"+b);
	
   if(a>b) {
	   System.out.println("The largest value is a:"+a);
   }
   else {
	   System.out.println("The largest value is b:"+b);
   }
   
}
}
